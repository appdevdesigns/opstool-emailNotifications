      <!-- Form Notification -->
      <div class="page-wrap mockup-display en-formNotification en-wizard-formNotification">
        <div class="container-fluid" id="frmNotification-setup">
          <div class="col-xs-12">
            <h2>Notification Setup</h2>
          </div><!-- /.col-xs-12 -->
          <div class="col-lg-4 col-md-5 col-sm-4 form-notifications">
            <!--<form role="form"> -->
              <div class="form-group">
                <label for="title">Notification Title</label>
                <input type="text" class="form-control" id="notificationTitle" name="notificationTitle" value="[[= data.attr('notificationTitle') ]]">
              </div>
              <div class="form-group">
                <label for="email">Email Subject</label>
                <input type="email" class="form-control" id="emailSubject" name="emailSubject">
              </div>
              <div class="form-group">
                <label for="name">From Name</label>
                <input type="text" class="form-control" id="fromName" name="fromName">
              </div>
              <div class="form-group">
                <label for="email">From Email</label>
                <input type="email" class="form-control" id="fromEmail" name="fromEmail">                
              </div>
           <!-- </form>-->
          </div><!-- /.form-notifications -->
          <div class="col-lg-7 col-md-6 col-sm-8 col-lg-offset-1 col-md-offset-1 form-tab">
            <p><strong>Select how you want to send notifications:</strong></p>          
            <div class="tabbable tabs-left">
              <ul class="nav nav-tabs">
                <li class="active"><a href="#basic" data-toggle="tab">Basic</a></li>
                <li><a href="#system" data-toggle="tab">System</a></li>
              </ul>
              <div class="tab-content">
                <div class="tab-pane active" id="basic">
                  <div class="tab-inner">
                    <h3 class="text-center">Basic</h3>
                    <div class="form-group">
                      <div class="col-lg-3 col-sm-5 col-md-4">
                        <label>Start from:*</label>
                      </div>
                      <div class="input-group date col-lg-6 col-md-5 col-sm-7" id="startFrom" >
                        <input type='text' class="form-control" id="dateStartFrom" name="startFrom" />
                        <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span></span>
                      </div>
                    </div>
                    <div class="form-group">
                      <div class="col-lg-3 col-sm-5 col-md-4">
                        <label>Repeat every:</label>
                      </div>
                      <div class="input-group date col-lg-6 col-md-5 col-sm-7 col-xs-12">
                        <select name="emailFrequency" id="emailFrequency" class="form-control">
                          <option value="Everyday">Everyday</option>
                          <option value="Monthly">Monthly</option>
                          <option value="Yearly">Yearly</option>
                        </select>
                      </div>
                    </div>                 
                    <div class="form-group">
					  <div class="col-lg-3 col-sm-5 col-md-4">
							<label>Repeat until:</label>
						</div>
						<div class="input-group date col-lg-6 col-md-5 col-sm-7" id="repeatUntil">
						   <input type='text' class="form-control" name="repeatUntil" id="dateRepeatUntil" />
						   <div class="form-control" id="no-date" style="display:none;">&nbsp;</div>
						   <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span></span>
						</div>
						<div class="block-never-end">
						   <input type='checkbox' name="neverEnd" id="neverEnd" /> Never End
						</div>
					 </div>
                    <div class="form-bottom">
                      <p>The Notification will be sent <span id="basic-settings"><a href="#">Everyday</a> from <span>1/12/2015</span> to <span>6/12/2015</span></span></p>
                      <a mockup-show="en-formTemplate" class="btn btn-default btn-black en-wiz-notifications-next">Next <i class="fa fa-arrow-right"></i></a>
                    </div><!-- /.form-bottom -->
                  </div><!-- /.tab-inner -->
                </div><!-- /.tab-pane -->
                <div class="tab-pane" id="system">
                  <div class="tab-inner tab-inner-system">
                    <h3 class="text-center">System</h3>
                    <div class="form-group">
                      <div class="input-group date col-lg-11 col-md-11 col-sm-11 col-lg-offset-1">
                        Select the event you want to use to trigger the notification being sent:
                      </div>
                    </div>
                    <div class="form-group">
                      <div class="input-group date col-lg-8 col-md-10 col-sm-10 col-lg-offset-1 col-xs-12">
                        <select class="form-control">
                          <option>13 Month Report Completed</option>
                        </select>
                      </div>
                    </div>
                    <div class="form-bottom">
                      <p>You are sending 13 month report</p>
                      <a mockup-show="en-formTemplate" class="btn btn-default btn-black en-wiz-notifications-system-next">Next <i class="fa fa-arrow-right"></i></a>
                    </div><!-- /.form-bottom -->
                  </div><!-- /.tab-inner -->
                </div><!-- /.tab-pane -->
              </div><!-- /.tab-content -->
            </div><!-- /.tabs-left -->
          </div><!-- /.form-tab -->
        </div><!-- /.container-fluid -->
      </div><!-- /.page-wrap -->
      <!-- End Form Notification -->
      
      <!-- Form Template -->
      <div class="page-wrap mockup-display en-formTemplate en-wizard-formTemplate">
			<div class="container-fluid">
				<div class="col-xs-12">
					<h2>Select a Template</h2>
				</div><!-- /.col-xs-12 -->
				<div class="col-lg-3 col-md-4 col-sm-5">
					<div class="panel-shadow text-center">
						<img src="/images/img-one-column-layout.png" alt="One Column Layout" />
						<h3>One Column</h3>
						<div class="panel-footer">
							<a mockup-show="en-selectDesign" class="btn btn-default btn-black en-wiz-select-template-next">Select</a>
						</div><!-- /.panel-footer -->
					</div>
				</div><!-- /.form-notifications -->
			</div><!-- /.container-fluid -->
		</div><!-- /.page-wrap -->
      <!-- End Form Template -->
      
      <!-- Template Design -->
      <div class="page-wrap mockup-display en-selectDesign en-wizard-selectDesign">
		<div class="container-fluid" id="frmDesign-setup">
			<div class="col-xs-12">
				<h2>Design a Template</h2>
			</div><!-- /.col-xs-12 -->
			<div class="col-lg-4 col-md-5 col-sm-4 form-wrapper">
				<!--<form role="form">-->
					<div class="form-group">
						<label for="title">Template Title</label>
						<input type="text" class="form-control" id="title" name="templateTitle">
					</div>						
				<!--</form>-->
			</div><!-- /.form-notifications -->
			<div class="col-lg-7 col-md-7 col-sm-8 col-lg-offset-1">
				<div class="textarea-section">
					<span>HTML</span>
					<div class="inner-text-box">
						<h4>Enter Your Text</h4>
						<textarea name="templateBody" id="templateBody"></textarea>							
					</div><!-- /.inner-text-box -->
					<div class="text-right">
						<a mockup-show="en-designConfirm" class="btn btn-default btn-black en-wiz-design-template-next">
							Next <i class="fa fa-arrow-right"></i>
						</a>
					</div><!-- /.text-right -->
				</div><!-- /.textarea-section -->
			</div><!-- /.form-tab -->
		</div><!-- /.container-fluid -->
	  </div><!-- /.page-wrap -->
      <!-- End Template Design -->
      
      <!-- Design Confirm -->
      <div class="page-wrap mockup-display en-designConfirm en-wizard-designConfirm">
		<div class="container-fluid">
			<div class="col-xs-12">
				<h2>Confirm Notification</h2>
			</div>
			<div class="col-lg-5 col-md-5 col-sm-12 block-confirm-notification">
				<p><strong>Please recheck the notification details:</strong></p>
				<ul>
					<li><strong>[[= numRecipients]] Recipients</strong> are selected</li>
					<li>Subject is "<strong>[[= notification.notificationSubject]]</strong>"</li>
					<li>Sending conditions based on "<strong>[[=notification.setupShortDesc]]</strong>"</li>
					<li>Template selected is "<strong>[[= notification.selectedTemplate]]</strong>"</li>
					<li>HTML text is given using the "<strong>[[= notification.templateType]]</strong>" Design</li>
				</ul>
			</div>
			<div class="col-lg-6 col-md-7 col-sm-12 col-lg-offset-1 textarea-confirm">
				<p><strong>This is the preview of the notification you are sending:</strong></p>
				<div class="textarea-section">						
					<div class="inner-text-box">
						<p class="subject-text"><em>Subject:</em> <strong>[[=notification.templateSubject]]</strong></p>
						<div class="confirm-box">[[=notification.templateBody]]</div>							
					</div>
					<div class="text-right confirm-bottom">
						<p>Please confirm your Notification</p>
						<a class="btn btn-default btn-black en-wiz-draft" obj-id="[[=notification.id]]">
							Draft <i class="fa fa-check"></i>
						</a>
						<a class="btn btn-confirm en-wiz-confirm" obj-id="[[=notification.id]]">
							<i class="fa fa-check"></i> Confirm
						</a>
					</div>
				</div>
			</div>
		</div>
	  </div>
      <!-- End Design Confirm -->
    </div>
    <!-- End Wizard -->
<!-- End HTML Mockup -->
 </div>
