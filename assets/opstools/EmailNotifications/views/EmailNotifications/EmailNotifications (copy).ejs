      <!-- Choose The Receipients panel -->
      <div class="page-wrap mockup-display en-wizard en-wizard-recipients">
        <div class="container-fluid">
		  <div class="col-xs-12">
            <h2>Recipient Lists</h2>
          </div>
          <div class="col-lg-5 col-md-5 col-sm-6 form-search">
            <div class="input-group">
              <input type="text" class="form-control en-recipient-search" placeholder="Search Here">
              <span class="input-group-btn">
                <button class="btn btn-default" type="button"><i class="fa fa-search"></i></button>
              </span>
            </div><!-- /input-group -->
          </div><!-- /.col-lg-6 -->
          <div class="col-lg-8 col-md-9 col-sm-12 recipient-list">
            <a href="#myModal" role="button" class="btn btn-list create-new-list" data-toggle="modal">Create New List</a>
            <div id="myModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
                  <div class="modal-dialog">
                    <div class="modal-content">
                      <div class="modal-header">
						<h4>Create New Recipient List</h4>
                          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">Ã—</button>
                      </div>
                      <div class="modal-body">
                       
							<div class="form-horizontal en-add-recipients-form">
                              <div class="form-group">
                                  <label class="control-label col-md-2 no-pd-right" for="title">List Title:</label>
                                  <div class="col-md-10">
                                    <input type="text" class="form-control" id="title" name="title" maxlength="200" placeholder="Enter List Title">
                                  </div>
                              </div>                        
                              <div class="form-group">
                                  <label class="control-label col-xs-12 text-left label-recipient" for="recipients">Email Recipients:</label>
                                  <div class="col-xs-12">
                                      <textarea rows="6" class="form-control" id="recipients" name="recipients" placeholder="Enter email addressesseprated by a comma (,)"></textarea>
									  <input type="hidden" name="recipient_id" id="recipient_id" value="">	
                                  </div>
                              </div>
                               <div class="form-group block-btn">
									<div class="col-md-12">
										<button type="submit" value="Save" class="btn btn-save en-recipient-list-save" id="save"> Save</button>
										<button type="submit" value="Cancel" class="btn btn-cancel en-recipient-btn-cancel" id="cancel"> Cancel</button>
									</div>
								</div>
                              </div>
                       
                      </div><!-- End of Modal body -->
                    </div><!-- End of Modal content -->
                  </div><!-- End of Modal dialog -->
              </div><!-- End of Modal -->
            <table class="table-notifications en-table-recipients" cellpadding="0" cellspacing="0" width="100%" data-height="300" data-toggle="table">
              <thead>
                <tr>
                  <th data-field="title" width="40%">List Title</th>
                  <th data-field="updatedAt" width="20%">Last Updated</th>
                  <th width="40%"></th>
                </tr>
              </thead> 
               <tbody>
               <!-- rows.forEach(function(recipient){ -->            
              <tr class="template" id="[[= recipient.id]]">              
                <td width="40%">[[= recipient.attr('title') ]]</td>
                <td width="30%">[[= new Date(recipient.attr('updatedAt')).toDateString() ]]</td>
                <td width="30%" class="text-center tmpl-options">
                  <a href="#" class="btn-modify" obj-id="[[=recipient.id]]"><i class="fa fa-pencil-square-o"></i><span>Modify</span></a>
                  <a href="#" class="btn-delete" obj-id="[[=recipient.id]]"><i class="fa fa-trash-o"></i> <span>Delete</span></a>
                </td>
              </tr>
                 <!-- }) -->
              <tr class="mockup">
                <td width="40%"><input type="checkbox" /> Finanace Team</td>
                <td width="20%">1/11/2015</td>
                <td width="40%" class="text-center">
                  <a href="#" class="btn-modify"><i class="fa fa-pencil-square-o"></i> <span>Modify</span></a>
                  <a href="#" class="btn-delete"><i class="fa fa-trash-o"></i> <span>Delete</span></a>
                </td>
              </tr>             
              </tbody>
            </table><!-- /.table-notifications -->
          </div><!-- /.col-xs-12 -->
